.PHONY: clean All

All:
	@echo "----------Building project:[ IfElseStatement - Debug ]----------"
	@cd "IfElseStatement" && "$(MAKE)" -f  "IfElseStatement.mk"
clean:
	@echo "----------Cleaning project:[ IfElseStatement - Debug ]----------"
	@cd "IfElseStatement" && "$(MAKE)" -f  "IfElseStatement.mk" clean
